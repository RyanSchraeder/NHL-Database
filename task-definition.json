{
    "containerDefinitions": [
        {
            "name": "nhl-data-pipeline",
            "image": "656379487547.dkr.ecr.us-east-1.amazonaws.com/nhl-data-pipeline",
            "essential": true,
            "entryPoint": [
                "bash",
                "-c"
            ],
            "command": [
                "prefect agent start -q nhl-data-pipeline"
            ],
            "environment": [
                {
                    "name": "SFUSER",
                    "value": "arn:aws:ssm:us-east-1:656379487547:parameter/SFUSER"
                },
                {
                    "name": "SFPW",
                    "value": "arn:aws:ssm:us-east-1:656379487547:parameter/SFPW"
                },
                {
                    "name": "SNOWFLAKE_ACCT",
                    "value": "arn:aws:ssm:us-east-1:656379487547:parameter/SNOWFLAKE_ACCT"
                },
                {
                    "name": "SFWH",
                    "value": "arn:aws:ssm:us-east-1:656379487547:parameter/SFWH"
                     SFSCHEMA
                },
                {
                    "name": "SFSCHEMA",
                    "value": "arn:aws:ssm:us-east-1:656379487547:parameter/SFSCHEMA"
                },
                {
                    "name": "SNOWFLAKE_DB",
                    "value": "arn:aws:ssm:us-east-1:656379487547:parameter/SNOWFLAKE_DB"
                }
            ],
            "environmentFiles": [],
            "mountPoints": [],
            "volumesFrom": [],
            "secrets": [
                {
                    "name": "PREFECT_WORKSPACE",
                    "valueFrom": "arn:aws:ssm:us-east-1:656379487547:parameter/PREFECT_WORKSPACE"
                },
                {
                    "name": "PREFECT_API_URL",
                    "valueFrom": "arn:aws:ssm:us-east-1:656379487547:parameter/PREFECT_API_URL"
                },
                {
                    "name": "PREFECT_API_KEY",
                    "valueFrom": "arn:aws:ssm:us-east-1:656379487547:parameter/PREFECT_API_KEY"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "nhl-data-pipeline",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "nhl-data-pipeline"
                }
            }
        }
    ],
    "family": "nhl-data-pipeline",
    "taskRoleArn": "arn:aws:iam::656379487547:role/nhl-data-pipeline_ecs_task_role",
    "executionRoleArn": "arn:aws:iam::656379487547:role/nhl-data-pipeline_ecs_execution_role",
    "networkMode": "awsvpc",
    "requiresAttributes": [
        {
            "name": "com.amazonaws.ecs.capability.logging-driver.awslogs"
        },
        {
            "name": "ecs.capability.execution-role-awslogs"
        },
        {
            "name": "com.amazonaws.ecs.capability.ecr-auth"
        },
        {
            "name": "com.amazonaws.ecs.capability.docker-remote-api.1.19"
        },
        {
            "name": "com.amazonaws.ecs.capability.docker-remote-api.1.17"
        },
        {
            "name": "com.amazonaws.ecs.capability.task-iam-role"
        },
        {
            "name": "ecs.capability.container-ordering"
        },
        {
            "name": "ecs.capability.execution-role-ecr-pull"
        },
        {
            "name": "ecs.capability.secrets.ssm.environment-variables"
        },
        {
            "name": "com.amazonaws.ecs.capability.docker-remote-api.1.18"
        },
        {
            "name": "ecs.capability.task-eni"
        }
    ],
    "placementConstraints": [],
    "compatibilities": [
        "EC2",
        "FARGATE"
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "512",
    "memory": "1024"
}
