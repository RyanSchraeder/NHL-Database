{
    "containerDefinitions": [
        {
            "name": "nhl_data_pipeline_ecs_container",
            "image": "656379487547.dkr.ecr.us-east-1.amazonaws.com/nhl_data_pipeline_ecr_repo",
            "essential": true,
            "entryPoint": [
                "bash",
                "-c"
            ],
            "command": [
                "prefect agent start -q nhl_data_pipeline_ecs_container"
            ],
            "secrets": [
                {
                    "name": "PREFECT_API_KEY",
                    "valueFrom": "arn:aws:ssm:us-east-1:656379487547:parameter/PREFECT_API_KEY"
                },
                {
                    "name": "SNOWFLAKE_DB",
                    "valueFrom": "arn:aws:ssm:us-east-1:656379487547:parameter/SNOWFLAKE_DB"
                },
                {
                    "name": "SFWH",
                    "valueFrom": "arn:aws:ssm:us-east-1:656379487547:parameter/SFWH"
                },
                {
                    "name": "PREFECT_WORKSPACE",
                    "valueFrom": "arn:aws:ssm:us-east-1:656379487547:parameter/PREFECT_WORKSPACE"
                },
                {
                    "name": "SFUSER",
                    "valueFrom": "arn:aws:ssm:us-east-1:656379487547:parameter/SFUSER"
                },
                {
                    "name": "SFPW",
                    "valueFrom": "arn:aws:ssm:us-east-1:656379487547:parameter/SFPW"
                },
                {
                    "name": "SNOWFLAKE_ACCT",
                    "valueFrom": "arn:aws:ssm:us-east-1:656379487547:parameter/SNOWFLAKE_ACCT"
                },
                {
                    "name": "SFSCHEMA",
                    "valueFrom": "arn:aws:ssm:us-east-1:656379487547:parameter/SFSCHEMA"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-create-group": "true",
                    "awslogs-group": "nhl_data_pipeline_ecs_container",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "nhl_pipeline"
                }
            }
        }
    ],
    "family": "nhl_data_pipeline_ecs_taskdefinition",
    "taskRoleArn": "arn:aws:iam::656379487547:role/AirflowECSTaskExecution",
    "executionRoleArn": "arn:aws:iam::656379487547:role/AirflowECSTaskExecution",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "512",
    "memory": "1024"
}
